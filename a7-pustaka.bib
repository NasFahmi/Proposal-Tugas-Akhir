  file     = {:C\:/Users/Fahmi/Downloads/PENGUJIAN+BLACK+BOX+TESTING+PADA+SISTEM+WEBSITE+PEMESANAN+ONLINE+TOKO+AYAM+KRISPY.pdf:pdf},
  journal  = {Jurnal Media Akademik (Jma)},
  number   = {5},
  pages    = {3031--5220},
  title    = {{PT. Media Akademik Publisher PENGUJIAN BLACK BOX TESTING PADA SISTEM WEBSITE PEMESANAN ONLINE TOKO AYAM KRISPY}},
