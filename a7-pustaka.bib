  issn     = {2963-6264},
  journal  = {Journal of Cooperative, Small and Medium Enterprise Development},
  number   = {2},
  pages    = {60--64},
  title    = {{Digitalisasi UMKM sebagai Upaya Peningkatan Pemasaran dan Penjualan Online di Desa Tengklik}},
  volume   = {1},
  year     = {2022}
