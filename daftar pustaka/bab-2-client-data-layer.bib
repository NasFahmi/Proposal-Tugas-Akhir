@article{Rahman2024,
  abstract = {Perkembangan teknologi web modern mendorong penggunaan framework seperti Next.js dalam pengembangan aplikasi. PT. Anugerah Wijaya Raga (AWR) mengembangkan aplikasi GGL (Gak Gendut Lagi) untuk manajemen nutrisi dan kebugaran, namun menghadapi tantangan kinerja terkait pemanggilan API yang tidak efisien dan berpengaruh ke respons aplikasi. Penelitian ini bertujuan mengoptimalkan kecepatan waktu respons aplikasi GGL dengan metode caching menggunakan Tanstack Query. Hasil menunjukkan bahwa caching efektif mengoptimalkan waktu respons aplikasi. Sebelum optimasi, waktu login hingga ke dashboard adalah 6,70 detik, setelah optimasi menjadi 4,43 detik (33,8%). Halaman mealplan meningkat dari 6 detik menjadi 3,94 detik (34,3%), dan halaman measurement dari 2,93 detik menjadi 1,47 detik (49,8%). Efektivitas pemanggilan API juga meningkat tanpa adanya duplikasi pemanggilan. Penerapan caching dan prefetching meningkatkan responsivitas aplikasi dan pengalaman pengguna secara keseluruhan.
              Kata Kunci: Next.js, Caching, Tanstack Query, Optimisasi, Aplikasi Fitness},
  author   = {Rahman, Abdul and Prihanto, Agus},
  doi      = {10.26740/jinacs.v6n02.p333-340},
  file     = {:D\:/Kuliah/RPL/Semester 7/Tugas Akhir/daftar pustaka/Optimisasi Kinerja Aplikasi Fitness Berbasis Next.js Melalui Penerapan Metode Caching Pada PT.Anugerah Wijaya Raga.pdf:pdf;:C\:/Users/Fahmi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rahman, Prihanto - 2024 - Optimisasi Kinerja Aplikasi Fitness Berbasis Next.js Melalui Penerapan Metode Caching Pada PT. Anugerah Wijaya.pdf:pdf},
  journal  = {Journal of Informatics and Computer Science (JINACS)},
  number   = {02},
  pages    = {333--340},
  title    = {{Optimisasi Kinerja Aplikasi Fitness Berbasis Next.js Melalui Penerapan Metode Caching Pada PT. Anugerah Wijaya Raga}},
  volume   = {6},
  year     = {2024}
}
